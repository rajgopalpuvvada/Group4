    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "SelfPhp\\": "config/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "rm -rf public/bootstrap/css",
            "rm -rf public/bootstrap/js",
            "cp -R vendor/twbs/bootstrap/dist/* public/bootstrap"
